{
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "modules",
    "typeRoots": [
      "typings"
    ],
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  }
}
